<h1>Test#index</h1>
<p>Find me in app/views/test/index.html.erb</p>


<form action="/test/post/" method="post">
  <input type="text" name="test"></input>
  <% @types.each do |type| %>
    <%= check_box_tag 'type[]', type.id %>
    <%= label_tag type.name %>
  <% end %>

  <input type="submit" value="post"></input>
</form>



